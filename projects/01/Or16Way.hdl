/**
 * 16-way or gate: out = in[0] or in[1] or ... or in[15].
 */

CHIP Or16Way {
    IN in[16];
    OUT out;

    PARTS:
      Or8Way(in=in[0..7],out=firsthalf);
      Or8Way(in=in[8..15],out=secondhalf);
      Or(a=firsthalf,b=secondhalf,out=out);
}
